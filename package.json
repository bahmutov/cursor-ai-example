{
  "name": "cursor-ai-example",
  "description": "Writing E2E tests with Cursor AI help example application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc --skipLibCheck --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "cypress:open": "cypress open --e2e",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:5173 cypress:run",
    "test:open": "start-server-and-test dev http://localhost:5173 cypress:open",
    "clean": "rm -rf .nyc_output coverage",
    "pretest:e2e": "npm run clean"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@tailwindcss/vite": "^4.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "redux-undo": "^1.1.0",
    "tailwindcss": "^4.0.14",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/node": "^22.13.13",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^14.2.1",
    "cypress-map": "^1.46.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-istanbul": "^7.0.0"
  }
}
